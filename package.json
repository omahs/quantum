    "@birthdayresearch/sticky-turbo": "^0.3.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0"
  },
  "engines": {
    "node": "^18.0.0 || ^16.16.0",
    "pnpm": ">=7.11.0"
  }
